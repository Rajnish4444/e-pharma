{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "e-pharma": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/e-pharma",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap/bootstrap.css",
              "src/assets/css/bootstrap/bootstrap-grid.css",
              "src/assets/css/bootstrap/bootstrap-reboot.css",
              "src/assets/css/aos.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/jquery-ui.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/owl.theme.default.min.css",
              "src/assets/css/style.css",
              "src/assets/scss/_site-base.scss",
              "src/assets/scss/_site-blocks.scss",
              "src/assets/scss/_site-navbar.scss",
              "src/assets/scss/style.scss"
            ],
            "scripts": [
              "src/assets/js/aos.js",
              "src/assets/js/bootstap.min.js",
              "src/assets/js/jquery-3.3.1.min.js",
              "src/assets/js/jquery-ui.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/main.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/slick.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "e-pharma:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "e-pharma:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "e-pharma:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "e-pharma:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "e-pharma:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "e-pharma",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}